<aura:component implements="force:appHostable,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" controller="EI_ZD_TenantRespondToClaim" access="global" >
    <aura:attribute name="claimId" type="string" default="" />
    <aura:attribute name="viewClaim" type="Boolean" default="true" />
    <aura:attribute name="showCancelDiv" type="Boolean" default="false" />
    <aura:attribute name="ViewContinue" type="Boolean" default="false" />
    <aura:attribute name="keyDocuments" type="Boolean" default="false" />
    <aura:attribute name="consentBoxValue" type="Boolean" default="false" />
    <aura:attribute name="showConsentError" type="Boolean" default="false" />
    <aura:attribute name="showClaimBreakdown" type="Boolean" default="false" />
    <aura:attribute name="showKeyDocumentsError" type="Boolean" default="false" />
    <aura:attribute name="showAdditionalComments" type="Boolean" default="false" />
    <aura:attribute name="showReviewsubmission" type="Boolean" default="false" />
    <aura:attribute name="isClaimForRent" type="Boolean" default="false" />
    <aura:attribute name="showDisputeItemError" type="Boolean" default="false" />
    
    <aura:attribute name="currentClaimCatagories" type="string" default="" />
    <aura:attribute name="currentItem" type="Integer" default="0" />
    <aura:attribute name="ClaimsDetails" type="list" />
    
    <aura:attribute name="isEditable" type="boolean" />
    
    <aura:attribute name="showConfirmDialog" type="boolean" default="false"/>
    <aura:attribute name="exceedCommentCharsCount" type="Integer" default="{!if(v.ClaimsDetails[0].Claim_exceeds_comment_AGLL__c, v.ClaimsDetails[0].Claim_exceeds_comment_AGLL__c.length, 0)}"/> <!--new added -->
    <aura:attribute name="errorMsg" type="string" default="" /> <!--new added -->
    <aura:attribute name="isTotalClaimGreaterThanGuaranteeAmt" type="boolean" default="false" /> <!--new added -->
    <aura:attribute name="excessClaimVal" type="Integer" /> <!--new added -->
    <aura:attribute name="formattedCurrency" type="String" /> <!--new added to test-->
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <!-- <aura:handler name="EI_ZD_refreshParentView" event="c:EI_ZD_refreshParentView" action="{!c.refreshParentViewEvent}"/> -->
    <aura:registerEvent name="EI_ZD_refreshParentView" type="c:EI_ZD_refreshParentView" />
    <body class="ZD_theme">
        <div id="scrollView121"> &nbsp;</div>
        
        
        <aura:if isTrue="{!v.viewClaim}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    <!-- Resolution Section HTML Start -->
                    <section class="resolution_section">
                        <div class="container">
                            <div>
                                <aura:if   isTrue="{!or((v.ClaimsDetails[0].Status =='Evidence gathering agent/landlord'),(and(v.ClaimsDetails[0].Status =='On Hold',v.ClaimsDetails[0].PreviousStatus__c=='Evidence gathering agent/landlord')))}"  >
                                    
                                    <aura:if isTrue="{!v.ClaimsDetails[0].AGLL_Respond_Evidance_Gathering__c}">
                                        <lightning:button variant="brand" label="Review/edit evidence" title="Review/edit evidence"
                                                          onclick="{! c.showContinue}" class="see-all"/> &nbsp;&nbsp;
                                        <aura:set attribute="else">
                                            <lightning:button variant="brand" label="Submit evidence" title="Submit evidence"
                                                              onclick="{! c.showContinue}" class="see-all"/> &nbsp;&nbsp;
                                        </aura:set>
                                    </aura:if>
                                    <aura:set attribute="else">
                                        <lightning:button variant="brand" label="Review evidence" title="Review evidence"
                                                          onclick="{! c.showContinue}" class="see-all"/> &nbsp;&nbsp;
                                    </aura:set>
                                </aura:if>
                                
                                <lightning:button variant="brand" label="Cancel claim " title="Cancel claim"
                                                  onclick="{! c.cancelClaim }" class="see-all"/> &nbsp;&nbsp;
                            </div>
                            <img src="{!$Resource.EI_ZD_Assets  + '/img/resolution.png'}" alt="Resolution" />
                        </div>
                    </section>
                    <!-- Resolution Section HTML End -->
                    <div class="deposit-agent">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-9">
                                    <!-- Table HTML Start -->
                                    <div class="table-container no_navigation ZD_custom_table">
                                        <table class="display scrollbar" style="width:100%">
                                            <tbody>
                                                <tr>
                                                    <td>Address</td>
                                                    <td>{!v.ClaimsDetails[0].Deposit_Account_Number__r.Property_Address__c}</td>
                                                </tr>
                                                <tr>
                                                    <td>Claim status</td>
                                                    <td>{!v.ClaimsDetails[0].Status=='On Hold' ? v.ClaimsDetails[0].PreviousStatus__c : v.ClaimsDetails[0].Status }</td>
                                                </tr>
                                                <tr>
                                                    <td>Tenancy start date</td>
                                                    <td> <ui:outputDate value="{!v.ClaimsDetails[0].Deposit_Account_Number__r.Start_Date__c}" format="dd/MM/yyyy"></ui:outputDate></td>
                                                </tr>
                                                <tr>
                                                    <td>Claim number</td>
                                                    <td>{!v.ClaimsDetails[0].External_ID__c}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <!-- Table HTML End -->
                                    
                                    <!-- Table HTML Start -->
                                    <div class="table-container no_navigation no_space">
                                        <table id="example-2" class="display scrollbar dataTable no-footer" style="width:100%">
                                            <thead>
                                                <tr>
                                                    <th>Name</th>
                                                    <th>Type</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <aura:iteration var="caseParticpnt" items="{!v.ClaimsDetails[0].Case_Participants__r}" indexVar="i">
                                                    <aura:if isTrue="{!caseParticpnt.Type__c =='Tenant'}">
                                                        <aura:if isTrue="{!(i+1)%2 == 0}">
                                                            <tr class="even">
                                                                <td> {!caseParticpnt.Contact__r.Name} </td>
                                                                <aura:if isTrue="{!caseParticpnt.Is_Lead__c}">
                                                                    <td> Lead tenant </td>
                                                                    <aura:set attribute="else">
                                                                        <td> Joint tenant </td>
                                                                    </aura:set>
                                                                </aura:if>
                                                                <!-- <td> {!caseParticpnt.Type__c}  </td> -->
                                                            </tr>
                                                        </aura:if>
                                                        <aura:if isTrue="{!(i+1)%2 != 0}">
                                                            <tr class="odd">
                                                                <td> {!caseParticpnt.Contact__r.Name} </td>
                                                                <aura:if isTrue="{!caseParticpnt.Is_Lead__c}">
                                                                    <td> Lead tenant </td>
                                                                    <aura:set attribute="else">
                                                                        <td> Joint tenant </td>
                                                                    </aura:set>
                                                                </aura:if>
                                                                <!-- <td> {!caseParticpnt.Type__c}  </td> -->
                                                            </tr>
                                                        </aura:if>
                                                    </aura:if>
                                                </aura:iteration>
                                            </tbody>
                                        </table>
                                    </div>
                                    <!-- Table HTML End -->
                                </div>
                                <aura:if isTrue="{!v.isEditable == true}">
                                    <div class="col-md-3 info-right-tab">
                                    <p>This page provides a summary of the claim, including where the claim is in the process.</p>
                                    <p>Please press 
                                        <aura:if isTrue="{!or((v.ClaimsDetails[0].Status =='Evidence gathering agent/landlord'),(and(v.ClaimsDetails[0].Status =='On Hold',v.ClaimsDetails[0].PreviousStatus__c=='Evidence gathering agent/landlord')))}" >
                                            
                                            <aura:if isTrue="{!v.ClaimsDetails[0].AGLL_Respond_Evidance_Gathering__c}">
                                                'Review/edit evidence'
                                                <aura:set attribute="else">
                                                    'Submit evidence'
                                                </aura:set>
                                            </aura:if>
                                            <aura:set attribute="else">
                                                'Review evidence'
                                            </aura:set>
                                        </aura:if>
                                        to provide the evidence that supports the claim.</p>
                                    <p><b>All tenants will be able to see the evidence you submit.</b></p>
                                    <p>For your information, we have listed the lead tenant. While communications go to all tenants, the lead tenant makes decisions on behalf of all tenants on how to proceed.</p>
                                    
                                </div>
                                    <aura:set attribute="else">
                                        <div class="col-md-3 info-right-tab">
                                            <p>Your information has now been made available to the tenant(s) and they will be asked to provide their response. The adjudicator will use all the information from both parties in making their decision.</p>
                                        </div>
                                    </aura:set>
                                </aura:if>
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>  
        </aura:if>
        <aura:if isTrue="{!v.showCancelDiv}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    
                    <div class="deposit-agent">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-9">
                                    <section class="deposit_steps">
                                        <div class="deposit_steps_detail">
                                            <div class="claim-summary_gray-panel border error">
                                                <h6>Claim Cancelled</h6>
                                                
                                                <p>Thank you for letting us know you wish to cancel this claim. We will close the case and let the tenant(s) know.</p>
                                            </div>
                                            <div class="text-right">
                                                <lightning:button class="see-all mw-auto" onclick="{!c.goToHomePage}" > Go to claim summary </lightning:button>
                                            </div>
                                        </div>
                                    </section>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </aura:if>
        <aura:if isTrue="{!v.ViewContinue}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    
                    <div class="list-of-btns">
                        <div class="container">
                            <lightning:button onclick="{!c.goToCliamSummary}" aura:id="div0" class="go-back"><img  src="{!$Resource.EI_ZD_Assets  + '/img/zd-arrow-dropleft.png'}" alt="Back Icon"/> Go back</lightning:button>
                            &nbsp;&nbsp;<lightning:button variant="brand" label="Cancel claim " title="Continue claim journey"
                                              onclick="{! c.cancelClaim }" class="see-all"/>
                        </div>
                    </div>
                    
                    <div class="deposit-agent">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-9">
                                    <section class="deposit_steps">
                                        <div class="deposit_steps_detail">
                                            <aura:if isTrue="{!v.isEditable==true}">
                                            <h6>You have until <!--<lightning:formattedDateTime value="{!v.ClaimsDetails[0].Respond_Date__c}" year="numeric" month="numeric" day="numeric" />--> <ui:outputDate value="{!v.ClaimsDetails[0].Respond_Date__c}" format="dd/MM/yyyy"></ui:outputDate> to submit evidence to support the claim.</h6>
                                            </aura:if>
                                                <div class="claim-summary_gray-panel">
                                                <h5>Claim summary</h5>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p>Amount claimed by agent/landlord</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                    </div>
                                                </div>
                                                <div class="row my-4">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p class="text-right">- of which is agreed by tenant</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p>(<lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/>)</p>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p><b>Amount outstanding</b></p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><b> <lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c - v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency" minimumFractionDigits="2" maximumFractionDigits="2"/></b></p>
                                                    </div>
                                                </div>
                                            </div>
                                            
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12">
                                                    <div class="agree-container">
                                                        <div class="agree-detail" style="display: block;">
                                                            <div class="form-group row">
                                                                <div class="col-sm-4">
                                                                    <div class="request-section">
                                                                        <label>Requested by you</label>
                                                                        <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                            <aura:if isTrue="{!desptItems.Type__c=='Other'}">
                                                                                <div class="form-group">
                                                                                    <label for="Others" class=" col-form-label">{!desptItems.Type__c}</label>
                                                                                    <div id="textarea"  contenteditable="true" disabled="true"  >
                                                                                        <div style="padding:2px; padding-right: 10px;">
                                                                                            <p id="ptag" disabled="true" > <lightning:formattedNumber value="{!desptItems.Agreed_by_AGLL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                                                            <p  id="ptag"  disabled="true" >{!desptItems.Other_Reason__c}</p>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <aura:set attribute="else">
                                                                                    <div class="form-group">
                                                                                        <label for="Cleaning" >{!desptItems.Type__c}</label>
                                                                                        <!-- <input type="number" class="form-control" disabled="true" id="Cleaning" placeholder="{!'£'+desptItems.Agreed_by_AGLL__c}"/> -->
                                                                                        <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_AGLL__c}" />
                                                                                        
                                                                                    </div>
                                                                                </aura:set>
                                                                            </aura:if>
                                                                        </aura:iteration>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4">
                                                                    <div class="response-section">
                                                                        <label>Previously agreed by tenant</label>
                                                                        <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                            <div class="form-group">
                                                                                <label >{!desptItems.Type__c}</label>
                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_Tenant__c}" />
                                                                                <!--<input type="number" disabled="true" class="form-control" placeholder="{!'£'+desptItems.Agreed_by_Tenant__c}"/> -->
                                                                            </div>
                                                                        </aura:iteration>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4" style="pointer-events: none;">
                                                                    <div class="dispute-section">
                                                                        <label>Amount still outstanding</label>
                                                                        <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                            <div class="form-group" style="cursor: not-allowed;">
                                                                                <label >{!desptItems.Type__c}</label>
                                                                                <lightning:input type="number"  name="input1"  formatter="currency" step="0.01"  value="{!sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}" />
                                                                                <!-- <input type="text" disabled="true" class="form-control" placeholder="{!'£'+sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}"/> -->
                                                                            </div>
                                                                        </aura:iteration>
                                                                    </div>
                                                                </div>
                                                                
                                                              <!--  <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                    <div class="col-sm-4">
                                                                        <div class="request-section">
                                                                            <aura:if isTrue="{!desptItems.Type__c=='Other'}">
                                                                                <div class="form-group">
                                                                                    <label for="Others" class=" col-form-label">{!desptItems.Type__c}</label>
                                                                                    <div id="textarea"  contenteditable="true" disabled="true"  >
                                                                                        <div style="padding:2px; padding-right: 10px;">
                                                                                            <p id="ptag" disabled="true" > <lightning:formattedNumber value="{!desptItems.Agreed_by_AGLL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                                                            <p  id="ptag"  disabled="true" >{!desptItems.Other_Reason__c}</p>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <aura:set attribute="else">
                                                                                    <div class="form-group">
                                                                                        <label for="Cleaning" >{!desptItems.Type__c}</label>
                                                                                        <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_AGLL__c}" />
                                                                                        
                                                                                    </div>
                                                                                </aura:set>
                                                                            </aura:if>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-sm-4">
                                                                        <div class="response-section">
                                                                            <div class="form-group">
                                                                                <label >{!desptItems.Type__c}</label>
                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_Tenant__c}" />
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-sm-4">
                                                                        <div class="dispute-section">
                                                                            <div class="form-group">
                                                                                <label >{!desptItems.Type__c}</label>
                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}" />
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </aura:iteration>
                                                                -->
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <aura:if isTrue ="{!v.isEditable}">   
                                                <h6>Please give your consent to the following:</h6>
                                                
                                                <p>I give authority to The Dispute Service to consider my claim and confirm that:</p>
                                                
                                                <ul class="custom-list">
                                                    <li>I accept responsibility for ensuring that any personal data contained in the information I submit complies with the Data Protection Act 2018 and the General Data Protection Regulation;</li>
                                                    <li>I consent to the information I provide being shared with the other party to the claim, the adjudicator and anyone else involved in resolving it;</li>
                                                </ul>
                                                
                                                <div class="form-row">
                                                    <div class="form-group col-md-12 text-right">
                                                        <div class="form-check">
                                                            <input type="checkbox" checked="{!v.ClaimsDetails[0].Consent_box_AGLL__c=='Yes'? true :false}" class="form-check-input" id="exampleCheck2"/>
                                                            <label class="form-check-label" for="exampleCheck2">I consent to the above</label>
                                                            <aura:if isTrue="{!v.showConsentError}">
                                                                <p class="error-msg text-right">
                                                                    You must give your authority for the claim to be considered. 
                                                                </p>
                                                            </aura:if>
                                                        </div>
                                                        
                                                    </div>
                                                </div>
                                            </aura:if>
                                            <div class="text-right">
                                                <lightning:button variant="brand" label="Continue" onclick="{!c.goToKeyDocuments}" class="see-all"/>
                                            </div>
                                        </div>
                                    </section>
                                    
                                </div>
                                
                                
                                <aura:if isTrue="{!v.isEditable == true}">
                                    <div class="col-md-3 info-right-tab">
                                        <p>This is your opportunity to provide details of your claim and evidence to support it. You can return to edit your response at any time up to <strong><ui:outputDate value="{!v.ClaimsDetails[0].Respond_Date__c}" format="dd/MM/yyyy"></ui:outputDate></strong>. During the process, your submission will be saved automatically each time you press ‘Continue’.</p>
                                        
                                        <p>After <strong><ui:outputDate value="{!v.ClaimsDetails[0].Respond_Date__c}" format="dd/MM/yyyy"></ui:outputDate></strong>, your information will be made available to the tenant(s) and they will be asked to provide their response. The adjudicator will use all the information from both parties in making their decision. You must provide all the information that you wish the adjudicator to use to reach their decision. You are unable to provide any further evidence after this, unless specifically requested by the adjudicator.</p>
                                        
                                        <p><b>Please respond fully to all the questions asked. It is your responsibility to explain your claim clearly and highlight the relevant evidence.</b></p>
                                        
                                        <p>You will be asked first whether you have some key documents. Please be aware that the success of your claim may be compromised if you are unable to provide them. If you do not have one or more of the key documents, please consider whether you wish to continue with your claim. You can opt to withdraw the claim at any point during the evidence submission process by selecting the option to ‘Cancel claim’.</p>
                                        
                                    </div>
                                    <aura:set attribute="else">
                                        <div class="col-md-3 info-right-tab">
                                            <p>Your information has now been made available to the tenant(s) and they will be asked to provide their response. The adjudicator will use all the information from both parties in making their decision.</p>
                                        </div>
                                    </aura:set>
                                </aura:if>
                            </div>
                        </div>
                    </div> 
                </div>
            </div>
        </aura:if>
        <aura:if isTrue="{!v.keyDocuments}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    
                    <div class="list-of-btns">
                        <div class="container">
                            <lightning:button onclick="{!c.goToCliamSummary}" aura:id="div1" class="go-back"><img  src="{!$Resource.EI_ZD_Assets  + '/img/zd-arrow-dropleft.png'}" alt="Back Icon"/> Go back</lightning:button>
                            &nbsp;&nbsp;<lightning:button variant="brand" label="Cancel claim " title="Continue claim journey"
                                              onclick="{! c.cancelClaim }" class="see-all"/>
                        </div>
                    </div>
                    
                    <div class="deposit-agent">
                        <div class="container" >
                            <div class="row">
                                <div class="col-md-9">
                                    
                                    <section class="deposit_steps">
                                        
                                        <div class="deposit_steps_count">
                                            <div class="state-indicator active">
                                                <span class="circle">1</span>
                                                <p>Key documents</p>
                                            </div>
                                            <div class="state-indicator">
                                                <span class="circle">2</span>
                                                <p>Claim breakdown</p>
                                            </div>
                                            <div class="state-indicator">
                                                <span class="circle">3</span>
                                                <p>Additional comments</p>
                                            </div>
                                            <div class="state-indicator">
                                                <span class="circle">4</span>
                                                <p>Review submission</p>
                                            </div>
                                        </div>
                                        
                                        <div class="deposit_steps_detail" >
                                            <form>
                                                <aura:if isTrue="{!v.ClaimsDetails[0].Claim_exceeds_the_total_level_of_cover__c}">
                                                    <div class="form-group row">
                                                        <div class="col-lg-12 col-md-12">
                                                            <div class="form-group">
                                                            <p>You have told us that your total claim exceeds the maximum guarantee amount. Using the fields below, please tell us the true value of your claim, along with any further comments that you wish to make regarding your excess claims. We recommend that you provide a full monetary breakdown of your claims in the reason box below. You can respond to individual claim areas in the next page(s). The maximum amount that the adjudicator can award is the maximum guarantee value.</p>
                                                                <lightning:input style="white-space: nowrap; margin-top: 8px;" disabled="{!!v.isEditable}" label="Total Claim Value" value="{!v.ClaimsDetails[0].Excess_Guarantee_Claim_Value__c}" formatter="currency" step="0.01" type="number" aura:id="totalclaimvalue" onchange="{!c.handleClaimValue}" required="true" class="input-container" placeholder="£"/>
                                                                
                                                                <!--<lightning:helptext content="Please tell us the true value of your claim."  />  -->
                                                                <aura:if isTrue="{!v.isTotalClaimGreaterThanGuaranteeAmt==true}">
